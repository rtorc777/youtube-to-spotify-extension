{
    "name": "Youtube to Spotify",
    "description": "Transfer songs from Youtube Music in videos to your Spotify playlists.",
    "version": "1.0.0",
    "manifest_version": 3,
    "icons": {
        "16": "assets/icon16.png",
        "32": "assets/icon32.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      },
    "background": {
        "service_worker":
            "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "identity",
        "scripting",
        "tabs"
    ]
}